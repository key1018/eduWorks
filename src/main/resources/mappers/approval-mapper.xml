<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ApprMapper">
<resultMap type="ApprForm" id="apprFormResultMap">
<id property="aprvNo" column="FORM_NO"></id>
<id property="memNo" column="FORM_NO"></id>
<id property="shelfLife" column="FORM_NO"></id>
<id property="secLevel" column="FORM_NO"></id>
<id property="type" column="FORM_NO"></id>
<id property="type" column="FORM_NO"></id>
<id property="type" column="FORM_NO"></id>
<id property="type" column="FORM_NO"></id>
<id property="type" column="FORM_NO"></id>
