<insert id="insertDraft">
insert
	into tb_aprv_draft
	(aprv_no
	,mem_no
	,aprv_draft_date
	,appv_draft_content
	,dept_code
	)
	values
	(
	#{aprvNo}
	,#{memNo}
	,#{draftDate}
	,#{draftContent}
	,#{deptCode}
	)
</insert>

</mapper>